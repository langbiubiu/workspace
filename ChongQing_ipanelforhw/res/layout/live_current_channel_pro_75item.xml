<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/RelativeLayout1"
    android:layout_width="550px"
    android:layout_height="105px"
    android:background="@drawable/live_list_channelpro_75focus"
    android:focusable="true" >

    <ImageView
        android:id="@+id/select_bg"
        android:layout_width="550px"
        android:layout_height="105px"
        android:visibility="invisible"
        android:background="@color/live_channel_list_select" />

    <RelativeLayout
        android:layout_width="550px"
        android:layout_height="105px"
        android:duplicateParentState="true"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/pro_time"
            android:layout_width="100px"
            android:layout_height="105px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="15px"
            android:duplicateParentState="true"
            android:gravity="center"
            android:text="@string/channel_back"
            android:textColor="@color/live_item_channelprotv_color"
            android:textSize="30px" />

        <TextView
            android:id="@+id/pro_name"
            android:layout_width="match_parent"
            android:layout_height="105px"
            android:layout_alignParentTop="true"
            android:layout_marginRight="68px"
            android:layout_toRightOf="@+id/pro_time"
            android:duplicateParentState="true"
            android:ellipsize="end"
            android:gravity="left|center_vertical"
            android:singleLine="true"
            android:textColor="@color/live_item_channelprotv_color"
            android:textSize="40px" />

        <ImageView
            android:id="@+id/live_pro_yy"
            android:layout_width="38px"
            android:layout_height="38px"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20px"
            android:duplicateParentState="true"
            android:background="@drawable/live_list_channelpro_yyfocus"
            android:gravity="center"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/live_pro_isp"
            android:layout_width="38px"
            android:layout_height="38px"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20px"
            android:background="@drawable/live_isp"
            android:gravity="center"
            android:visibility="invisible" />
    </RelativeLayout>

</FrameLayout>