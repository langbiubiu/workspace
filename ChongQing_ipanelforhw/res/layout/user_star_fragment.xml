<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="1692px"
    android:layout_height="1080px" >

    <com.ipanel.join.cq.user.UserTitle
        android:id="@+id/user_star_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="910px"
        android:layout_y="58px" >
    </com.ipanel.join.cq.user.UserTitle>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="1430px"
        android:layout_y="58px"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/user_star_curpage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="32px" />

        <TextView
            android:id="@+id/user_star_totalpage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="32px" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/hang"
            android:textSize="32px" />
    </LinearLayout>

    <TextView
        android:id="@+id/star"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="97px"
        android:layout_y="52px"
        android:text="@string/user_actor"
        android:textSize="48px"
        android:visibility="gone" />

    <android.support.v4.view.VerticalViewPager2
        android:id="@+id/star_gd"
        android:layout_width="1473px"
        android:layout_height="837px"
        android:layout_x="60px"
        android:layout_y="143px" />

</AbsoluteLayout>