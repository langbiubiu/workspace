<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="1692px"
    android:layout_height="1080px" >

    <com.ipanel.join.cq.user.UserTitle
        android:id="@+id/user_history_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="910px"
        android:layout_y="58px" >
    </com.ipanel.join.cq.user.UserTitle>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="1430px"
        android:layout_y="58px"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/user_history_curpage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="32px" />

        <TextView
            android:id="@+id/user_history_totalpage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="32px" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/hang"
            android:textSize="32px" />
    </LinearLayout>

    <android.support.v4.view.VerticalViewPager2
        android:id="@+id/user_history_viewpager"
        android:layout_width="1473px"
        android:layout_height="970px"
        android:layout_centerInParent="true"
        android:layout_x="72px"
        android:layout_y="63px" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="1017px"
        android:layout_x="82px"
        android:layout_y="63px"
        android:background="@drawable/user_line"
        android:visibility="invisible" />

</AbsoluteLayout>